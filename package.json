{
  "name": "chu-tools-client",
  "version": "1.0.0",
  "description": "Front-end Stack with Pug, Webpack, Babel, ES2016, Jest",
  "author": "Gairal",
  "readmeFilename": "README.md",
  "main": "src/app/index.tsx",
  "repository": {
    "type": "git",
    "url": "git@github.com:gairal/chu-tools-client.git"
  },
  "license": "MIT",
  "keywords": [
    "Front-end",
    "Pug",
    "css",
    "JavaScript",
    "Webpack",
    "Typescript",
    "es6",
    "es2015",
    "jest"
  ],
  "scripts": {
    "start": "npx webpack-dev-server --config ./webpack/webpack.config.js",
    "debug": "npx webpack-dev-server --config ./webpack/webpack.config.debug.js",
    "build": "npx webpack --config ./webpack/webpack.config.js",
    "deploy": "npx firebase deploy --only hosting --project com-gairal-chools",
    "release": "npx webpack --config ./webpack/webpack.config.prod.js",
    "lint": "npx tslint './src/app/**/*.{ts,tsx}' --project tsconfig.json && npx tslint ./test/ --project tsconfig.json && npx stylelint './src/**/*.scss'",
    "test": "npm run lint",
    "format": "npx prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3",
    "@commitlint/config-conventional": "^8.3",
    "@hot-loader/react-dom": "^16.11",
    "@types/history": "^4.7",
    "@types/node": "^13.5",
    "@types/query-string": "^6.3",
    "@types/react": "^16.9",
    "@types/react-dates": "^17.1",
    "@types/react-dom": "^16.9",
    "@types/react-redux": "^7.1",
    "@types/react-router-dom": "^5.1",
    "@types/redux-actions": "^2.6",
    "autoprefixer": "^9.7",
    "awesome-typescript-loader": "^5.2",
    "clean-webpack-plugin": "^3.0",
    "copy-webpack-plugin": "^5.1",
    "css-loader": "^3.4",
    "cssnano": "^4.1",
    "file-loader": "^5.0",
    "firebase-tools": "^7.12",
    "html-webpack-plugin": "^3.2",
    "husky": "^4.2",
    "imagemin-webpack-plugin": "^2.4",
    "mini-css-extract-plugin": "^0.9",
    "node-sass": "^4.13",
    "postcss-loader": "^3.0",
    "postcss-reporter": "^6.0",
    "prettier": "^1.19",
    "pug": "^2.0",
    "pug-loader": "^2.4",
    "sass-loader": "^8.0",
    "stylelint": "^13.0",
    "stylelint-config-concentric-order": "^3.5",
    "stylelint-config-sass-guidelines": "^6.2",
    "stylelint-order": "^4.0",
    "stylelint-scss": "^3.13",
    "stylelint-selector-bem-pattern": "^2.1",
    "tslint": "^6.00",
    "tslint-config-airbnb": "^5.11",
    "tslint-config-prettier": "^1.18",
    "tslint-loader": "^3.5",
    "tslint-react": "^4.2",
    "typescript": "^3.7",
    "uglifyjs-webpack-plugin": "^2.2",
    "webpack": "^4.41",
    "webpack-bundle-analyzer": "^3.6",
    "webpack-cli": "^3.3",
    "webpack-dev-server": "^3.10",
    "write-file-webpack-plugin": "^4.5"
  },
  "dependencies": {
    "connected-react-router": "^6.6",
    "es6-promise": "^4.2",
    "firebase": "^7.7",
    "history": "^4.10",
    "moment": "^2.24",
    "query-string": "^6.10",
    "react": "^16.12",
    "react-bar-loader": "^1.0",
    "react-dates": "^21.7",
    "react-dom": "^16.12",
    "react-firebaseui": "^4.1",
    "react-hot-loader": "^4.12",
    "react-key-handler": "^1.2.0-beta.3",
    "react-redux": "^7.1",
    "react-router": "^5.1",
    "react-router-dom": "^5.1",
    "react-switch": "^5.0",
    "redux": "^4.0",
    "redux-saga": "^1.1",
    "tailwindcss": "^1.1",
    "typesafe-actions": "^5.1",
    "whatwg-fetch": "^3.0"
  }
}
